											"referenceName": "ADLSSINK_CSV",
											"type": "DatasetReference",
											"parameters": {
												"container": {
													"value": "@item().container_sink",
													"type": "Expression"
												},
												"directory": {
													"value": "@concat(item().tablename_source,'/DatePart-',formatDateTime(utcnow(),'yyyy-MM-dd'))",
													"type": "Expression"
												},
